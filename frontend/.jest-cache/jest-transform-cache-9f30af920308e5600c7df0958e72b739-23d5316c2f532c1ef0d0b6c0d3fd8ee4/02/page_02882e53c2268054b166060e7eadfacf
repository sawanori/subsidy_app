        /* istanbul ignore next */
        cov_1t5xfkpr11().s[42]++;
        setTimeout(() => {
          /* istanbul ignore next */
          cov_1t5xfkpr11().f[5]++;
          cov_1t5xfkpr11().s[43]++;
          router.push('/ja/login?registered=true');
        }, 1500);
      } else
      /* istanbul ignore next */
      {
        cov_1t5xfkpr11().b[7][1]++;
      }
    } catch (err) {
      /* istanbul ignore next */
      cov_1t5xfkpr11().s[44]++;
      console.error('Signup error:', err);
      /* istanbul ignore next */
      cov_1t5xfkpr11().s[45]++;
      setError('アカウント作成中にエラーが発生しました。もう一度お試しください。');
    } finally {
      /* istanbul ignore next */
      cov_1t5xfkpr11().s[46]++;
      setIsLoading(false);
    }
  };
  /* istanbul ignore next */
  cov_1t5xfkpr11().s[47]++;
  return /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
    className: "flex min-h-screen items-center justify-center p-4",
    children: /*#__PURE__*/(0, _jsxruntime.jsxs)(_card.Card, {
      className: "w-full max-w-md",
      children: [/*#__PURE__*/(0, _jsxruntime.jsxs)(_card.CardHeader, {
        className: "space-y-1",
        children: [/*#__PURE__*/(0, _jsxruntime.jsx)(_card.CardTitle, {
          className: "text-2xl font-bold text-center",
          children: "アカウント作成"
        }), /*#__PURE__*/(0, _jsxruntime.jsx)(_card.CardDescription, {
          className: "text-center",
          children: "補助金申請システムのアカウントを作成します"
        })]
      }), /*#__PURE__*/(0, _jsxruntime.jsxs)("form", {
        onSubmit: handleSubmit,
        children: [/*#__PURE__*/(0, _jsxruntime.jsxs)(_card.CardContent, {
          className: "space-y-4",
          children: [
          /* istanbul ignore next */
          (cov_1t5xfkpr11().b[8][0]++, error) &&
          /* istanbul ignore next */
          (cov_1t5xfkpr11().b[8][1]++, /*#__PURE__*/(0, _jsxruntime.jsx)(_alert.Alert, {
            variant: "destructive",
            className: "alert",
            children: /*#__PURE__*/(0, _jsxruntime.jsx)(_alert.AlertDescription, {
              children: error
            })
          })),
          /* istanbul ignore next */
          (cov_1t5xfkpr11().b[9][0]++, success) &&
          /* istanbul ignore next */
          (cov_1t5xfkpr11().b[9][1]++, /*#__PURE__*/(0, _jsxruntime.jsx)(_alert.Alert, {
            className: "alert",
            children: /*#__PURE__*/(0, _jsxruntime.jsx)(_alert.AlertDescription, {
              children: "アカウントを作成しました。確認メールをご確認ください。"
            })
          })), /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
            className: "space-y-2",
            children: [/*#__PURE__*/(0, _jsxruntime.jsx)(_label.Label, {
              htmlFor: "accountName",
              children: "アカウント名"
            }), /*#__PURE__*/(0, _jsxruntime.jsx)(_input.Input, {
              id: "accountName",
              type: "text",
              placeholder: "株式会社サンプル",
              value: formData.accountName,
              onChange: e => {
                /* istanbul ignore next */
                cov_1t5xfkpr11().f[6]++;
                cov_1t5xfkpr11().s[48]++;
                return setFormData({
                  ...formData,
                  accountName: e.target.value
                });
              },
              required: true,
              disabled: isLoading
            })]
          }), /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
            className: "space-y-2",
            children: [/*#__PURE__*/(0, _jsxruntime.jsx)(_label.Label, {
              htmlFor: "email",
              children: "メールアドレス"
            }), /*#__PURE__*/(0, _jsxruntime.jsx)(_input.Input, {
              id: "email",
              type: "email",
              placeholder: "example@company.com",
              value: formData.email,
              onChange: e => {
                /* istanbul ignore next */
                cov_1t5xfkpr11().f[7]++;
                cov_1t5xfkpr11().s[49]++;
                return setFormData({
                  ...formData,
                  email: e.target.value
                });
              },
              required: true,
              disabled: isLoading
            })]
          }), /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
            className: "space-y-2",
            children: [/*#__PURE__*/(0, _jsxruntime.jsx)(_label.Label, {
              htmlFor: "password",
              children: "パスワード"
            }), /*#__PURE__*/(0, _jsxruntime.jsx)(_input.Input, {
              id: "password",
              type: "password",
              placeholder: "8文字以上で入力",
              value: formData.password,
              onChange: e => {
                /* istanbul ignore next */
                cov_1t5xfkpr11().f[8]++;
                cov_1t5xfkpr11().s[50]++;
                return setFormData({
                  ...formData,
                  password: e.target.value
                });
              },
              required: true,
              disabled: isLoading,
              minLength: 8
            })]
          }), /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
            className: "space-y-2",
            children: [/*#__PURE__*/(0, _jsxruntime.jsx)(_label.Label, {
              htmlFor: "confirmPassword",
              children: "パスワード（確認）"
            }), /*#__PURE__*/(0, _jsxruntime.jsx)(_input.Input, {
              id: "confirmPassword",
              type: "password",
              placeholder: "パスワードを再入力",
              value: formData.confirmPassword,
              onChange: e => {
                /* istanbul ignore next */
                cov_1t5xfkpr11().f[9]++;
                cov_1t5xfkpr11().s[51]++;
                return setFormData({
                  ...formData,
                  confirmPassword: e.target.value
                });
              },
              required: true,
              disabled: isLoading,
              minLength: 8
            })]
          })]
        }), /*#__PURE__*/(0, _jsxruntime.jsxs)(_card.CardFooter, {
          className: "flex flex-col space-y-4",
          children: [/*#__PURE__*/(0, _jsxruntime.jsxs)(_button.Button, {
            type: "submit",
            className: "w-full",
            disabled: isLoading,
            children: [
            /* istanbul ignore next */
            (cov_1t5xfkpr11().b[10][0]++, isLoading) &&
            /* istanbul ignore next */
            (cov_1t5xfkpr11().b[10][1]++, /*#__PURE__*/(0, _jsxruntime.jsx)(_icons.Icons.spinner, {
              className: "mr-2 h-4 w-4 animate-spin"
            })), "アカウントを作成"]
          }), /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
            className: "text-sm text-center text-muted-foreground",
            children: ["既にアカウントをお持ちですか？", ' ', /*#__PURE__*/(0, _jsxruntime.jsx)(_link.default, {
              href: "/ja/login",
              className: "text-primary hover:underline",
              children: "ログイン"
            })]
          })]
        })]
      })]
    })
  });
}
