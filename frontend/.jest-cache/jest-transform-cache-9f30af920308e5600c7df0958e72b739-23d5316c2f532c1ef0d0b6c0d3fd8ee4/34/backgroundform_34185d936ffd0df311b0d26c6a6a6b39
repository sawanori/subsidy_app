    /* istanbul ignore next */
    cov_2llqx0bgzr().b[4][0]++;
    cov_2llqx0bgzr().s[23]++;
    return {
      default: obj
    };
  } else
  /* istanbul ignore next */
  {
    cov_2llqx0bgzr().b[4][1]++;
  }
  var cache =
  /* istanbul ignore next */
  (cov_2llqx0bgzr().s[24]++, _getRequireWildcardCache(nodeInterop));
  /* istanbul ignore next */
  cov_2llqx0bgzr().s[25]++;
  if (
  /* istanbul ignore next */
  (cov_2llqx0bgzr().b[7][0]++, cache) &&
  /* istanbul ignore next */
  (cov_2llqx0bgzr().b[7][1]++, cache.has(obj))) {
    /* istanbul ignore next */
    cov_2llqx0bgzr().b[6][0]++;
    cov_2llqx0bgzr().s[26]++;
    return cache.get(obj);
  } else
  /* istanbul ignore next */
  {
    cov_2llqx0bgzr().b[6][1]++;
  }
  var newObj =
  /* istanbul ignore next */
  (cov_2llqx0bgzr().s[27]++, {
    __proto__: null
  });
  var hasPropertyDescriptor =
  /* istanbul ignore next */
  (cov_2llqx0bgzr().s[28]++,
  /* istanbul ignore next */
  (cov_2llqx0bgzr().b[8][0]++, Object.defineProperty) &&
  /* istanbul ignore next */
  (cov_2llqx0bgzr().b[8][1]++, Object.getOwnPropertyDescriptor));
  /* istanbul ignore next */
  cov_2llqx0bgzr().s[29]++;
  for (var key in obj) {
    /* istanbul ignore next */
    cov_2llqx0bgzr().s[30]++;
