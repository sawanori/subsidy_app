    /* istanbul ignore next */
    (cov_2llqx0bgzr().b[10][0]++, key !== "default") &&
    /* istanbul ignore next */
    (cov_2llqx0bgzr().b[10][1]++, Object.prototype.hasOwnProperty.call(obj, key))) {
