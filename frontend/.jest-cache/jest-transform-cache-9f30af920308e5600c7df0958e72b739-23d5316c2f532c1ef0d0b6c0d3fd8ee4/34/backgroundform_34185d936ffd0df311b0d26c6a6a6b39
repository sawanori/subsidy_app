        /* istanbul ignore next */
        cov_2llqx0bgzr().b[12][0]++;
        cov_2llqx0bgzr().s[33]++;
        Object.defineProperty(newObj, key, desc);
      } else {
        /* istanbul ignore next */
        cov_2llqx0bgzr().b[12][1]++;
        cov_2llqx0bgzr().s[34]++;
        newObj[key] = obj[key];
      }
    } else
    /* istanbul ignore next */
    {
      cov_2llqx0bgzr().b[9][1]++;
    }
  }
  /* istanbul ignore next */
  cov_2llqx0bgzr().s[35]++;
  newObj.default = obj;
  /* istanbul ignore next */
  cov_2llqx0bgzr().s[36]++;
  if (cache) {
    /* istanbul ignore next */
    cov_2llqx0bgzr().b[14][0]++;
    cov_2llqx0bgzr().s[37]++;
    cache.set(obj, newObj);
  } else
  /* istanbul ignore next */
  {
    cov_2llqx0bgzr().b[14][1]++;
  }
  cov_2llqx0bgzr().s[38]++;
  return newObj;
}
