    /* istanbul ignore next */
    (cov_1juhb4ro51().b[10][0]++, key !== "default") &&
    /* istanbul ignore next */
    (cov_1juhb4ro51().b[10][1]++, Object.prototype.hasOwnProperty.call(obj, key))) {
      /* istanbul ignore next */
      cov_1juhb4ro51().b[9][0]++;
      var desc =
      /* istanbul ignore next */
      (cov_1juhb4ro51().s[31]++, hasPropertyDescriptor ?
