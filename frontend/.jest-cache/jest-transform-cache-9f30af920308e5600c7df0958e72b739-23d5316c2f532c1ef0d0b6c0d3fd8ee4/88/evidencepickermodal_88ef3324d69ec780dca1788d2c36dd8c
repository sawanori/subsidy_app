                    children: [/*#__PURE__*/(0, _jsxruntime.jsx)(_lucidereact.Sparkles, {
                      className: "h-3 w-3"
                    }), "AI推奨"]
                  }))]
                })]
              }), /*#__PURE__*/(0, _jsxruntime.jsx)(_button.Button, {
                variant: "ghost",
                size: "sm",
                onClick: () => {
                  /* istanbul ignore next */
                  cov_1juhb4ro51().f[26]++;
                  cov_1juhb4ro51().s[111]++;
