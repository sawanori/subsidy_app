      /* istanbul ignore next */
      (cov_1juhb4ro51().b[13][0]++, desc) && (
      /* istanbul ignore next */
      (cov_1juhb4ro51().b[13][1]++, desc.get) ||
      /* istanbul ignore next */
      (cov_1juhb4ro51().b[13][2]++, desc.set))) {
        /* istanbul ignore next */
        cov_1juhb4ro51().b[12][0]++;
        cov_1juhb4ro51().s[33]++;
        Object.defineProperty(newObj, key, desc);
      } else {
        /* istanbul ignore next */
        cov_1juhb4ro51().b[12][1]++;
        cov_1juhb4ro51().s[34]++;
        newObj[key] = obj[key];
      }
    } else
    /* istanbul ignore next */
    {
      cov_1juhb4ro51().b[9][1]++;
    }
  }
  /* istanbul ignore next */
  cov_1juhb4ro51().s[35]++;
  newObj.default = obj;
  /* istanbul ignore next */
  cov_1juhb4ro51().s[36]++;
  if (cache) {
    /* istanbul ignore next */
    cov_1juhb4ro51().b[14][0]++;
    cov_1juhb4ro51().s[37]++;
    cache.set(obj, newObj);
  } else
  /* istanbul ignore next */
  {
    cov_1juhb4ro51().b[14][1]++;
  }
  cov_1juhb4ro51().s[38]++;
  return newObj;
}
