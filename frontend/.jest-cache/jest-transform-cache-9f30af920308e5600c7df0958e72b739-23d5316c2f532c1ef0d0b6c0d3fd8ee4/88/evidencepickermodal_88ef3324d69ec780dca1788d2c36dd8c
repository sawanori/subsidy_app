                    }), "ページ ", paragraph.pageNumber]
                  })),
                  /* istanbul ignore next */
                  (cov_1juhb4ro51().b[33][0]++, paragraph.lineNumber) &&
                  /* istanbul ignore next */
                  (cov_1juhb4ro51().b[33][1]++, /*#__PURE__*/(0, _jsxruntime.jsxs)("span", {
                    className: "flex items-center gap-1",
                    children: [/*#__PURE__*/(0, _jsxruntime.jsx)(_lucidereact.Hash, {
                      className: "h-3 w-3"
                    }), "行 ", paragraph.lineNumber]
                  })),
                  /* istanbul ignore next */
                  (cov_1juhb4ro51().b[34][0]++, paragraph.confidence) &&
                  /* istanbul ignore next */
                  (cov_1juhb4ro51().b[34][1]++, /*#__PURE__*/(0, _jsxruntime.jsxs)(_badge.Badge, {
                    variant: paragraph.confidence >= 0.9 ?
                    /* istanbul ignore next */
                    (cov_1juhb4ro51().b[35][0]++, 'default') :
                    /* istanbul ignore next */
                    (cov_1juhb4ro51().b[35][1]++, paragraph.confidence >= 0.7 ?
                    /* istanbul ignore next */
                    (cov_1juhb4ro51().b[36][0]++, 'secondary') :
                    /* istanbul ignore next */
                    (cov_1juhb4ro51().b[36][1]++, 'outline')),
