  if (
  /* istanbul ignore next */
  (cov_1juhb4ro51().b[5][0]++, obj === null) ||
  /* istanbul ignore next */
  (cov_1juhb4ro51().b[5][1]++, typeof obj !== "object") &&
  /* istanbul ignore next */
  (cov_1juhb4ro51().b[5][2]++, typeof obj !== "function")) {
    /* istanbul ignore next */
    cov_1juhb4ro51().b[4][0]++;
    cov_1juhb4ro51().s[23]++;
    return {
      default: obj
    };
  } else
  /* istanbul ignore next */
  {
    cov_1juhb4ro51().b[4][1]++;
  }
  var cache =
  /* istanbul ignore next */
  (cov_1juhb4ro51().s[24]++, _getRequireWildcardCache(nodeInterop));
  /* istanbul ignore next */
  cov_1juhb4ro51().s[25]++;
  if (
  /* istanbul ignore next */
  (cov_1juhb4ro51().b[7][0]++, cache) &&
  /* istanbul ignore next */
  (cov_1juhb4ro51().b[7][1]++, cache.has(obj))) {
    /* istanbul ignore next */
    cov_1juhb4ro51().b[6][0]++;
    cov_1juhb4ro51().s[26]++;
    return cache.get(obj);
  } else
  /* istanbul ignore next */
  {
    cov_1juhb4ro51().b[6][1]++;
  }
  var newObj =
  /* istanbul ignore next */
  (cov_1juhb4ro51().s[27]++, {
    __proto__: null
  });
  var hasPropertyDescriptor =
  /* istanbul ignore next */
  (cov_1juhb4ro51().s[28]++,
  /* istanbul ignore next */
  (cov_1juhb4ro51().b[8][0]++, Object.defineProperty) &&
  /* istanbul ignore next */
  (cov_1juhb4ro51().b[8][1]++, Object.getOwnPropertyDescriptor));
