                      return toggleExpanded(error.code);
                    },
                    children: isExpanded ?
                    /* istanbul ignore next */
                    (/*#__PURE__*/cov_1p1cytx7vt().b[44][0]++, (0, _jsxruntime.jsx)(_lucidereact.ChevronDown, {
                      className: "h-4 w-4"
                    })) :
                    /* istanbul ignore next */
                    (/*#__PURE__*/cov_1p1cytx7vt().b[44][1]++, (0, _jsxruntime.jsx)(_lucidereact.ChevronRight, {
                      className: "h-4 w-4"
                    }))
                  })), /*#__PURE__*/(0, _jsxruntime.jsx)(_button.Button, {
                    variant: "ghost",
                    size: "sm",
                    onClick: () => {
                      /* istanbul ignore next */
                      cov_1p1cytx7vt().f[20]++;
                      cov_1p1cytx7vt().s[107]++;
                      return navigator.clipboard.writeText(`Error: ${error.code}\nMessage: ${error.message}\nTime: ${error.timestamp}`);
                    },
                    children: /*#__PURE__*/(0, _jsxruntime.jsx)(_lucidereact.Copy, {
                      className: "h-3 w-3"
                    })
                  })]
                })]
              }),
              /* istanbul ignore next */
              (cov_1p1cytx7vt().b[45][0]++, error.details) &&
              /* istanbul ignore next */
              (cov_1p1cytx7vt().b[45][1]++, isExpanded) &&
              /* istanbul ignore next */
              (cov_1p1cytx7vt().b[45][2]++, /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
                className: "mt-2 p-2 bg-muted rounded text-xs font-mono",
                children: /*#__PURE__*/(0, _jsxruntime.jsx)("pre", {
                  children: JSON.stringify(error.details, null, 2)
                })
              }))]
            }, `${error.code}-${index}`);
          })
        })
      })
    })]
  });
}
