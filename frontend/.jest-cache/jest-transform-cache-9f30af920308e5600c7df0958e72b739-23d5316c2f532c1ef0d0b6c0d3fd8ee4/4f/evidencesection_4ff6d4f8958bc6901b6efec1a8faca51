            })
          }))]
        })]
      })]
    }),
    /* istanbul ignore next */
    (cov_25e8my3u29().b[28][0]++, selectedEvidence) &&
    /* istanbul ignore next */
    (cov_25e8my3u29().b[28][1]++, /*#__PURE__*/(0, _jsxruntime.jsx)(_evidencepickermodal.EvidencePickerModal, {
