{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^@prisma/client$": "@prisma/client",
    "^@prisma/prisma\\.service$": "<rootDir>/../src/prisma/prisma.service",
    "^@prisma/prisma\\.module$": "<rootDir>/../src/prisma/prisma.module",
    "^@/(.*)$": "<rootDir>/../src/$1",
    "^@generated/(.*)$": "<rootDir>/../generated/$1",
    "^@common/(.*)$": "<rootDir>/../src/common/$1",
    "^@modules/(.*)$": "<rootDir>/../src/modules/$1",
    "^@evidence/(.*)$": "<rootDir>/../src/evidence/$1",
    "^@applications/(.*)$": "<rootDir>/../src/applications/$1",
    "^@plans/(.*)$": "<rootDir>/../src/plans/$1"
  }
}