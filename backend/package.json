  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "node -r tsconfig-paths/register dist/src/main",
    "start:dev": "ts-node -T -r tsconfig-paths/register src/main.ts",
    "start:debug": "ts-node --inspect-brk -r tsconfig-paths/register src/main.ts",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "seed:schemes": "ts-node -r tsconfig-paths/register prisma/seed-schemes.ts"
  },
